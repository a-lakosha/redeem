<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Redeem Digital Wallet Theme - Menu Configuration -->

    <!-- Note: This theme is designed as a single landing page -->
    <!-- Additional menu items can be added here if more pages are needed -->

    <!-- Example menu items (commented out for single-page design) -->

    <record id="menu_features" model="website.menu">
        <field name="name">Feature</field>
        <field name="url">/#features</field>
        <field name="parent_id" ref="website.main_menu"/>
        <field name="sequence" type="int">20</field>
    </record>

    <record id="menu_how_it_works" model="website.menu">
        <field name="name">How It Works</field>
        <field name="url">/#how-it-works</field>
        <field name="parent_id" ref="website.main_menu"/>
        <field name="sequence" type="int">30</field>
    </record>


</odoo>